<package>
  <name>jsk_unitree_startup</name>
  <version>1.1.0</version>
  <description>
     jsk_unitree_startup
  </description>
  <maintainer email="k-okada@jsk.t.u-tokyo.ac.jp">Kei Okada</maintainer>
  <license>BSD</license>
  <url>http://ros.org/wiki/jsk_unitree_startup</url>
  <buildtool_depend>catkin</buildtool_depend>

  <run_depend>jsk_robot_startup</run_depend>
  <run_depend>unitree_legged_real</run_depend>
  <run_depend>rwt_app_chooser</run_depend>
  <run_depend>app_manager</run_depend>
  <run_depend>rosserial_python</run_depend>
  <run_depend>teleop_twist_joy</run_depend>
  <run_depend>respeaker_ros</run_depend>
  <run_depend>google_chat_ros</run_depend>

  <!-- for diagnostic aggregator -->
  <run_depend>diagnostic_aggregator</run_depend>

  <test_depend>roslaunch</test_depend>
  <test_depend>roslint</test_depend>
  <test_depend>rostest</test_depend>

  <export>
    <app_manager app_dir="${prefix}/apps"/>
  </export>
</package>
