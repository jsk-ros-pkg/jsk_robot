<launch>
  <arg name="username" default="dummyusername" />
  <arg name="password" default="dummypassword" />
  <arg name="hostname" default="192.168.50.3" />

  <!-- spot driver -->
  <include
      file="$(find jsk_spot_startup)/launch/include/driver.launch"
      >
      <arg name="credential_config" value="$(arg credential_config)" />
      <arg name="launch_robot_state_publisher" value="$(arg launch_robot_state_publisher)" />
  </include>

  <!-- peripheral devices -->
  <include
      file="$(find jsk_spot_startup)/launch/include/peripheral.launch"
      />

  <!-- interaction nodes -->
  <include
      file="$(find jsk_spot_startup)/launch/include/interaction.launch"
      />

</launch>
