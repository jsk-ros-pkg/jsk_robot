<launch>
  <!-- strelka -->
  <arg name="google_chat_credentials_json" default="$(find jsk_spot_startup)/auth/tribal-quasar-343006-1025e6fc703f.json" />
  <arg name="google_cloud_credentials_json" default="$(find jsk_spot_startup)/auth/eternal-byte-236613-4bc6962824d1.json" />
  <!-- <arg name="google_chat_credentials_json" default="$(find jsk_spot_startup)/auth/strelka-di9f-a0fb121b63de.json" /> -->
  <!-- belka -->
  <!-- <arg name="google_chat_credentials_json" default="$(find jsk_spot_startup)/auth/belka-xc9u-244cc72fdda0.json" /> -->
  <arg name="dialogflow_credentials_json" default="$(find jsk_spot_startup)/auth/facialexpression-rpwe-6b3a00d46db8.json" />

  <!-- Google Chat ROS -->
  <include file="$(find google_chat_ros)/launch/google_chat.launch">
    <arg name="receiving_mode" value="pubsub" />
    <!-- <arg name="project_id" value="spot-jsk-xc9u" /> <!-\- belka -\-> -->
    <arg name="project_id" value="tribal-quasar-343006" />
    <!-- <arg name="project_id" value="strelka-di9f" /> <!-\- strelka -\-> -->
    <arg name="subscription_id" value="chat-sub" />
    <arg name="respawn" value="true" />
    <arg name="google_cloud_credentials_json" value="$(arg google_chat_credentials_json)"/>
    <arg name="to_dialogflow_client" value="true" />
    <arg name="send_dialogflow_response_to_google_chat" value="false" />
  </include>

  <!-- Dialogflow Client ROS -->
  <include file="$(find dialogflow_task_executive)/launch/dialogflow_ros.launch">
    <arg name="credential" value="$(arg dialogflow_credentials_json)" />
    <arg name="project_id" value="facialexpression-rpwe" />
    <!-- <arg name="project_id" value="collaborative-remembering-kwit" /> -->
    <arg name="enable_hotword" value="false" />
  </include>

  <!-- Google Cloud Language -->
  <include file="$(find ros_google_cloud_language)/launch/analyze_text.launch">
    <arg name="google_cloud_credentials_json" value="$(arg google_cloud_credentials_json)" />
  </include>

  <!-- gdrive_ros -->
  <include file="$(find gdrive_ros)/launch/gdrive_server.launch">
    <arg name="settings_yaml" value="$(find jsk_spot_startup)/auth/pydrive_settings.yaml" />
    <arg name="respawn" value="true" />
    <arg name="node_name" value="gdrive_ros" />
  </include>

</launch>
